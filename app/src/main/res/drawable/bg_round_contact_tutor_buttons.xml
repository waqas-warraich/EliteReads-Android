<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <stroke android:color="@color/disable_state_color" />
            <solid android:color="@color/teal_regular_15_opacity" />
            <corners android:radius="@dimen/_4sdp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/orange_losos_15_opacity" />
            <solid android:color="@color/orange_losos_15_opacity" />
            <corners android:radius="@dimen/_4sdp" />
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1.0dip" android:color="@color/colorPrimary" />
            <solid android:color="@color/signUp_outline_btn_bg_color_1" />
            <corners android:radius="@dimen/_4sdp" />
        </shape>
    </item>
</selector>